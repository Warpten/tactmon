    "boost-container-hash",
    "boost-core",
    "boost-filesystem",
    "boost-iostreams",
    "boost-program-options",
    "boost-range",
    "boost-thread",
    "openssl",
    "zlib"
  ],
  "builtin-baseline": "7c729f5fcdd13ca5384f3053dd957e82c10ac2d5"
}
