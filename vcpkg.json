{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "description": "libtactmon - a Blizzard CDN monitor library",
    "name": "tactmon",
    "version-string": "latest",
    "features": {
        "frontend": {
            "description": "A self-hosted tact monitor with an HTTP and a Discord frontend.",
            "dependencies": [
                "libpqxx",
                "dpp"
            ]
        }
    },
    "dependencies": [
        "spdlog",
        "boost-algorithm",
        "boost-asio",
        "boost-beast",
        "boost-container",
        "boost-container-hash",
        "boost-core",
        "boost-filesystem",
        "boost-iostreams",
        "boost-program-options",
        "boost-range",
        "boost-thread",
        "openssl",
        "zlib"
    ]
}
